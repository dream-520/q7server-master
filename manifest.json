{
    "manifest_version": 2,
    "name": "q7-server-master",
    "version": "0.1.0",
    "description": "运维有些地方有点难用",
    "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_title": "save your life to check server info",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/jquery-1.8.3.js", "js/sm.js"],
            "run_at": "document_start"
        }
    ],
    "permissions": [
        "webRequest",
        "storage",
        "cookies",
        "http://*/*",
        "https://*/*"
    ],
    "homepage_url": "http://ops.q7link.com:8080/",
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "omnibox": {"keyword": "qs"}

}